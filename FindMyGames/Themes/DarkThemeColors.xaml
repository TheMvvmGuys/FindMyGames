<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TheMvvmGuys.FindMyGames"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <SolidColorBrush x:Key="WindowBackgroundColor" po:Freeze="true" Color="#494949"/>
    <SolidColorBrush x:Key="WindowForegroundColor" po:Freeze="True" Color="White"/>
    <SolidColorBrush x:Key="ControlBorderColor" po:Freeze="True" Color="#D2D2D2"/>
    <SolidColorBrush x:Key="ControlBackgroundColor" po:Freeze="True" Color="#585858"/>
    <SolidColorBrush x:Key="ControlFocusedBackgroundColor" po:Freeze="True" Color="#6A6A6A"/>
    <SolidColorBrush x:Key="ControlClickedBackgroundColor" po:Freeze="True" Color="Gray"/>
    <SolidColorBrush x:Key="ControlDisabledBackgroundColor" po:Freeze="True" Color="#A8A8A8"/>
    <SolidColorBrush x:Key="ControlDisabledBorderColor" po:Freeze="True" Color="DimGray"/>
</ResourceDictionary>